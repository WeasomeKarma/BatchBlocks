if %cspell%==11 set cssname=healing
if %cspell%==12 set cssname=sapping
Echo %itemload%. %it% of %cssname%
Echo It has %is1s% uses.
Echo It has a spell damage %is2s%
if %itemload% LSS %itemnum% goto itemloadf
set /p choice="Which item do you use?"
if %choice%==1 set /a itemuse=1
if %choice%==2 set /a itemuse=2
if %choice%==3 set /a itemuse=3
if %choice%==4 set /a itemuse=4
(
set /p it=
set /p cspell=
set /p is1s=
set /p is2s=
)<item%itemuse%.bat
Echo Your item beguins to glow!
pause
set /a is1s=%is1s%-1
del item%itemuse%.bat
(
Echo %it%
Echo %cspell%
Echo %is1s%
Echo %is2s%
)>item%itemuse%.bat
set /a itemusing=1
set is2s=%is2s%
if %it%==scroll set /a bonus=85
if %it%==wand set /a bonus=65
if %it%==orb set /a bonus=25
set /a ran=%RANDOM% %%99 + 1 + %is2s%
if %ran% gtr %bonus% goto spellauto
if %cspell%==1 goto firebolt
