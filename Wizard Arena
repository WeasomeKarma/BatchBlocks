if %choice%==1 set /a GatherRenown= %GatherRenown% + 1
if %choice%==2 set /a ReactionSpeed= %Reactionspeed% + 1
if %choice%==3 set /a Learner= %Learner% + 1
if %choice%==4 set /a Tricker= %Tricker% + 1
if %choice%==5 set /a Empower= %Empower% + 1
if %choice%==6 set /a Imagination= %Imagination% + 1
if %choice%==7 set /a Begging= %Begging% + 1
if %choice%==8 set /a Observer= %Observer% + 1
if %choice%==9 set /a Stealth= %Stealth% + 1
if %choice%==10 set /a luck= %luck% + 1
set /a SP= %SP% -1
if %SP% GTR 0 goto creation
