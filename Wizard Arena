set /a fac= %random% %%4 + %cr% + 10
set /a fss1= %random% %%12
set /a fss2= %random% %%12
set /a fss3= %random% %%12
set blurused=0
Echo You enter the battle ring!
if 20 GTR %renown% Echo Not many people pay attention... Must be an intermission.
if 20 LSS %renown% goto r1
goto r2
:r1
if %renown% LSS 40 Echo A lot of people are eager to watch you in your fight!
if %renown% GTR 80 Echo People roar in delight as you enter the ring awaiting to see your glorious skills of combat!
:r2
pause
echo Do you attempt to gather more attention?
set /p choice="(y/n)"
if %choice%==n goto r3
set /a roll= %RANDOM% %%20 + %GatherRenown% + %cha% + %fs2% + %bonus1%
set /a var= %RANDOM% %%20 + %cr%
if %roll% GTR %var% set /a renown= %renown% + 1
if %roll% GTR %var% Echo You impress the observers!
if %roll% GTR %var% set /a itc=%itc% + 1
if %roll% LSS %var% Echo You fail to impress the crowd...
:r3
pause
Echo Out comes your opponent...
set /a frace=%RANDOM% %%5 + 1
if %frace%==1 set frace=Human
if %frace%==2 set frace=Orc
if %frace%==3 set frace=Elf
if %frace%==4 set frace=Dwarf
if %frace%==5 set frace=Goblin
if %frace%==6 set frace=Undead
set /a hair= %RANDOM% %%3 + 1
set /a eyesl= %RANDOM% %%4 + 1
set /a eyesr= %RANDOM% %%4 + 1
set /a ears= %RANDOM% %%3 + 1
set /a nose= %RANDOM% %%6 + 1
