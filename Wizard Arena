set /p choice="(1-15)"
if %choice%==1 set /a SP=%sp% +1
if %choice%==2 set /a HP=%hp%+2
if %choice%==3 set /a XP=%xp%+2
if %choice%==4 set /a str=%str% + 1
if %choice%==5 set /a AC=%AC% + 1
if %choice%==6 goto hutest
if %choice%==7 goto hutest2
if %choice%==8 goto octest
if %choice%==9 goto octest2
if %choice%==10 goto eltest
if %choice%==11 goto eltest2
if %choice%==12 goto dwtest
if %choice%==13 goto dwtest2
if %choice%==14 goto gobtest
if %choice%==15 goto gobtest2
goto racialdone
:hutest
if %race%==6 goto hutest1
if NOT %race%==1 goto racial
:hutest1
set /a dex=%dex%+1
goto racialdone
:octest
