Echo [LEFT EYE]
Echo   \
Echo    [_ "large"= {[0]}
Echo    [_ "small"= .{0}.
Echo    [_ "wide"= .-0-.
Echo    [_ "non-existant"= (MMM)
set /p peyesl=choice
Echo [RIGHT EYE]
Echo   \
Echo    [_ "large"= {[0]}
Echo    [_ "small"= .{0}.
Echo    [_ "wide"= .-0-.
Echo    [_ "non-existant"= (MMM)
set /p peyesr=choice
Echo [NOSE]
Echo   \
Echo    [_ "very_long"= [ ] 
Echo    [               \ /
Echo    [                M
Echo    [_ "long"= [ ]
Echo    [          \ /
Echo    [
Echo    [_ "small"= [_]
Echo    [
Echo    [_ "upturned"= [=]
Echo    [
Echo    [_ "wide"= /=\
Echo    [          \M/
set /p pnose=
Echo [EARS]
Echo   \
Echo    [_ "large"= /\
Echo    [           []
Echo    [           {}
Echo    [
Echo    [_ "small"= __
Echo    [           []
Echo    [           \/
set /p pears=
echo HAIR
set /p phair=
echo BEARD
set /p pbeard=
:playerracedone
if %peyesl%==large set peyesl={[0]}
if %peyesl%==small set peyesl=.{0}.
if %peyesl%==wide set peyesl=.-0-.
if %peyesl%==non-existant set peyesl=(MMM)
if %peyesr%==large set peyesr={[0]}
if %peyesr%==small set peyesr=.{0}.
if %peyesr%==wide set peyesr=.-0-.
if %peyesr%==non-existant set peyesr=(MMM)
set pear1=  
set pear2=  
set pear3=  
set pnose1=/-\
set pnose2=---
set pnose3=__
set pbeard2=.
if %pears%==large set pear1=/\
if %pears%==large set pear2=[]
if %pears%==large set pear3={}
if %pears%==small set pear1=__
if %pears%==small set pear2=[]
if %pears%==small set pear3=\/
if %phair%==long echo                        MMMMMM
if %phair%==long echo                     MMMMMMMMMMMM
if %phair%==long echo                    MMMMMMMMMMMMMM
if %phair%==short echo                      mmmmmmmmmm
if %phair%==short echo                    mmmmmmmmmmmmmm
if %phair%==non-existant echo                    /------------\
if %phair%==long echo                  %pear1%MM          MM%pear1%
if %phair%==short echo                  %pear1%m            m%pear1%
if %phair%==non-existant echo                  %pear1%[            ]%pear1%
echo                  %pear2%[%peyesl%  %peyesr%]%pear2%
if %pnose%==very_long set pnose1=[ ]
if %pnose%==very_long set pnose2=\ /
if %pnose%==very_long set pnose3=M_
if %pnose%==long set pnose1=[ ]
if %pnose%==long set pnose2=\_/
if %pnose%==small set pnose1=[_]
if %pnose%==upturned set pnose1=[=]
if %pnose%==wide set pnose1=/=\
if %pnose%==wide set pnose2=\M/
if %pbeard%==non-existant set pbeard1=\_____%pnose3%_____/
if %pbeard%==large set pbeard1=\MMMMM%pnose3%MMMMM/
if %pbeard%==small set pbeard1=\mmmmm%pnose3%mmmmm/
echo                  %pear3%[    %pnose1%     ]%pear3%
echo                    [ ---%pnose2%---  ]
echo                    %pbeard1%
if %race%==1 goto phumanbody
if %race%==2 goto porcbody
if %race%==3 goto pelfbody
