if %eyesr%==2 set eyesr=small
if %eyesr%==3 set eyesr=wide
if %eyesr%==4 set eyesr=non-existant
if %ears%==1 set ears=large
if %ears%==1 set ears=large
if %ears%==2 set ears=small
if %ears%==3 set ears=non-existant
