if %nose%==2 set nose=long
if %nose%==3 set nose=small
if %nose%==4 set nose=upturned
if %nose%==5 set nose=wide
if %nose%==6 set nose=non-existant
if %fat%==1 set fat=fat
if %fat%==2 set fat=medium_fatness
if %fat%==3 set fat=skinny_as_a_twig
if %frace%==6 set fat=fleshless
if %buff%==1 set buff=giant
if %buff%==2 set buff=large
if %buff%==3 set buff=medium
if %buff%==4 set buff=small
if %buff%==5 set buff=tiny
if %beard%==1 set beard=large
if %beard%==2 set beard=small
if %beard%==3 set beard=non-existant
Echo He is a %buff% sized, %fat% %frace% who has %hair% hair, a %eyesl% left eye and a %eyesr% right eye, with a %nose% nose, with a set of %ears% ears, and a %beard% beard.
echo .
echo .
echo .
if %eyesl%==large set eyesl={[0]}
if %eyesl%==small set eyesl=.{0}.
if %eyesl%==wide set eyesl=.-0-.
if %eyesl%==non-existant set eyesl=(MMM)
if %eyesr%==large set eyesr={[0]}
if %eyesr%==small set eyesr=.{0}.
if %eyesr%==wide set eyesr=.-0-.
if %eyesr%==non-existant set eyesr=(MMM)
set ear1=  
set ear2=  
set ear3=  
set nose1=/-\
set nose2=---
set nose3=__
set beard2=.
if %ears%==large set ear1=/\
if %ears%==large set ear2=[]
if %ears%==large set ear3=\/
if %ears%==small set ear1=__
if %ears%==small set ear2=[]
if %ears%==small set ear3=++
if %hair%==long echo                        MMMMMM
if %hair%==long echo                     MMMMMMMMMMMM
if %hair%==long echo                    MMMMMMMMMMMMMM
if %hair%==short echo                      mmmmmmmmmm
if %hair%==short echo                    mmmmmmmmmmmmmm
if %hair%==non-existant echo                    /------------\
if %hair%==long echo                  %ear1%MM          MM%ear1%
if %hair%==short echo                  %ear1%m            m%ear1%
if %hair%==non-existant echo                  %ear1%[            ]%ear1%
echo                  %ear2%[%eyesl%  %eyesr%]%ear2%
if %nose%==very_long set nose1=[ ]
if %nose%==very_long set nose2=\ /
if %nose%==very_long set nose3=M_
if %nose%==long set nose1=[ ]
if %nose%==long set nose2=\_/
if %nose%==small set nose1=[_]
if %nose%==upturned set nose1=[=]
if %nose%==wide set nose1=/=\
if %nose%==wide set nose2=\M/
if %beard%==non-existant set beard1=\_____%nose3%_____/
if %beard%==large set beard1=\MMMMM%nose3%MMMMM/
if %beard%==small set beard1=\mmmmm%nose3%mmmmm/
echo                  %ear3%[    %nose1%     ]%ear3%
echo                    [ ---%nose2%---  ]
echo                    %beard1%
if %frace%==Human goto humanbody
if %frace%==Orc goto orcbody
if %frace%==Elf goto elfbody
if %frace%==Dwarf goto dwarfbody
