if %starttre%==8 set starttre=%stre8%
if %starttre%==9 set starttre=%stre9%
if %endtre%==1 set endtre=%etre1%
if %endtre%==2 set endtre=%etre2%
if %endtre%==3 set endtre=%etre3%
if %endtre%==4 set endtre=%etre4%
if %endtre%==5 set endtre=%etre5%
if %endtre%==6 set endtre=%etre6%
if %endtre%==7 set endtre=%etre7%
if %endtre%==8 set endtre=%etre8%

set /a fhp=%RANDOM% %%12 + %cr% + %cr%
echo You walk up behind your selected victim with a shiv in your hand...
pause
set /a ran=%RANDOM% %%19 + 1 + %stealth%
set /a crran=%RANDOM% %%19 + 1 + %cr%
if %ran% gtr %crran% goto crkills
set /a hitran=%RANDOM% %%10 + 1
if %hitran%==1 set limb=left_foot
if %hitran%==2 set limb=right_foot
if %hitran%==3 set limb=left_hand
if %hitran%==4 set limb=right_hand
if %hitran%==5 set limb=left_arm
if %hitran%==6 set limb=right_arm
if %hitran%==7 set limb=left_leg
if %hitran%==8 set limb=right_leg
if %hitran%==9 set limb=chest
if %hitran%==10 set limb=neck
if %hitran%==11 set limb=head
echo You go to strike your opponment in the %limb%
set /a damage= %RANDOM% %%5 + %str% + %hitran% + %level%
echo Your opponemnt sees your incomming blow and ignores increased dammage due to stealth!
echo You deal %damage% damage!
pause
set /a fhp=%fhp% - %damage%
if %fhp% lss 1 goto crkkill
set /a ran=%RANDOM% %%2 + 1
if %ran%==1 goto crkrun
if %ran%==2 goto crkfightpre
:crkills
set /a hitran=%RANDOM% %%10 + 1
if %hitran%==1 set limb=left_foot
if %hitran%==2 set limb=right_foot
if %hitran%==3 set limb=left_hand
if %hitran%==4 set limb=right_hand
if %hitran%==5 set limb=left_arm
if %hitran%==6 set limb=right_arm
if %hitran%==7 set limb=left_leg
if %hitran%==8 set limb=right_leg
if %hitran%==9 set limb=chest
if %hitran%==10 set limb=neck
if %hitran%==11 set limb=head
echo You go to strike your opponment in the %limb%
set /a damage= %RANDOM% %%5 + %str% + %hitran% + %level% + %stealth% * 2
echo Your opponemnt does not see your incomming blow! He gets dealt plus 2x stealth damage!
echo You deal %damage% damage!
pause
set /a fhp=%fhp% - %damage%
if %fhp% lss 1 goto crkkill
set /a ran=%RANDOM% %%2 + 1
if %ran%==1 goto crkrun
if %ran%==2 goto crkfightpre
:crkrun
echo Foe - I am %foename% %fperson%!
Echo "Please don't kill me!"
pause
goto crkfight
:crkfightpre
color 4
cls
echo Foe - I am %foename% %fperson%!!
pause
echo %foename% - You! You %startins% %endins%!
pause
echo %foename% - I will rip your %starttre% and %endtre% them
pause
cls
color %setcolor%
goto crkfight
:crkfight
set /a fAC=10 + %cr% - %RANDOM% %%4
set /a thp=%hp%
:crkfoe
if %fhp% lss 1 goto crkkill
set /a fitem= %RANDOM% %%4 + 1
if %fitem%==1 set fitemt=Knife
if %fitem%==2 set fitemt=Brick
if %fitem%==3 set fitemt=Saw
if %fitem%==4 set fitemt=Arcane_Hammer
if %fitem%==5 set fitemt=Sparkstone
echo Your opponment picks up a %fitemt% and attempts to hit you with it!
set /a fhit=%RANDOM% %%19 + 1 + %cr%
pause
if %fhit% GTR %AC% goto crkfhit
Echo He misses!
pause
goto crkplayer
:crkfhit
set /a hitran=%RANDOM% %%10 + 1
if %hitran%==1 set limb=left_foot
if %hitran%==2 set limb=right_foot
if %hitran%==3 set limb=left_hand
if %hitran%==4 set limb=right_hand
if %hitran%==5 set limb=left_arm
if %hitran%==6 set limb=right_arm
if %hitran%==7 set limb=left_leg
if %hitran%==8 set limb=right_leg
if %hitran%==9 set limb=chest
if %hitran%==10 set limb=neck
if %hitran%==11 set limb=head
Echo He hits you in the %limb%.
pause
if %fitemt%==Saw set itembonus=1
if %fitemt%==Arcane_Hammer set itembonus=5
if %fitemt%==Sparkstone set itembonus=10
set /a damage=%RANDOM% %%10 + %itembonus%
echo He deals %damage% damage!
set /a thp=%thp%-%damage%
pause
if %thp% LSS 1 goto crkdead
:crkplayer
if %crkdead%==2 goto ddead
if %crkdead%==1 set crkdead=2
Echo Its your turn!
Echo 1. Attack
Echo 2. Flee
Echo What do you do? (1-2)
set /p choice=
if %choice%==1 goto crkplayerhit
goto crkflee
:crkflee
Echo You flee from the fight and return to the waiting room.
Echo You are then sent to battle...
pause
goto battle
:crkplayerhit
Echo You attempt to strike him with the shiv!
set /a hit=%RANDOM% %%19 + 1 + %str% + %level%
if %hit% gtr %fAC% goto crkplayerhithit
Echo You miss!
pause
goto crkfoe
:crkplayerhithit
Echo You hit him!
set /a damage= %RANDOM% %%5 + %str% + %level%
echo %damage%
set /a fhp=%fhp%-%damage%
pause
goto crkfoe
:cgkill
set choice=0
pause
Echo You walk up to the person you belive is your oppoment...
Echo 1. Kill him
Echo 2. Ask who he is.
Echo 3. Walk away.
Echo What do you do?(1-3)
set /p choice=
if %choice%==1 goto crkill
if %choice%==2 goto cgkillask
if %choice%==3 goto craftpick
:cgkillgo
goto crkfight
:crkdead
set /a choice=0
cls
Echo You fall onto your back to the crafting room floor...
Echo You are slowly bleeding out.
Echo What do you do?
Echo 1. Beg for your life.
Echo 2. Attempt to do one more blow.
Echo 3. Give into the pain.
set /p choice=
if %choice%==1 goto crkbeg
if %choice%==2 set /a crkdie=1
if %choice%==3 goto ddead
goto crkplayer
:crkbeg
set /a ran=%RANDOM% %%19 + 1 + %Begger%
set /a ran2=%RANDOM% %%19 + 1 + %cr%
if %ran% lss %ran2% goto crkbegfail
Echo Your foe walks away, letting you live.
pause
goto crkflee
:crkbegfail
Echo Your foe picks up his chair and smashes the leg of it into your eye.
pause
Ehco You vision fades to black as the blood flows out of your eye soaking into the chair leg's wood.
pause
goto ddead
:cgkillask
Echo You ask your possible foe who he is.
pause
Echo He says,
set /a frace=%RANDOM% %%5 + 1
if %frace%==1 set frace=Human
if %frace%==2 set frace=Elf
if %frace%==3 set frace=Orc
if %frace%==4 set frace=Dwarf
if %frace%==5 set frace=Goblin
if %frace%==6 set frace=Undead
set /a ranchance=%RANDOM% %%4 + 1
if %ranchance%==1 Echo "Oh hello there. I am an Illusionist %frace%."
if %ranchance%==2 Echo "Oh hello there. I am an Destruction %frace%."
if %ranchance%==3 Echo "Oh hello there. I am an Necromancer %frace%."
if %ranchance%==1 set fperson=Illusionist_%frace%
if %ranchance%==2 set fperson=Destruction_%frace%
if %ranchance%==3 set fperson=Necromancer_%frace%
if %ranchance%==4 Echo "What do you want? I am a %frace%."
if %ranchance%==5 Echo "Go away! I am very busy!"
pause
if %ranchance%==1 set /a cgkillask=10
if %ranchance%==2 set /a cgkillask=20
if %ranchance%==3 set /a cgkillask=5
if %ranchance%==4 set /a cgkillask=-10
if %ranchance%==5 set /a cgkillask=-40
if %cgkillask%==10 Echo You think he could be your oppoment...
if %cgkillask%==20 Echo You highly think he could be your oppoment...
if %cgkillask%==5 Echo Your not very sure if hes your oppoment...
if %cgkillask%==-10 Echo There is a fair chance he is not your oppoment...
if %cgkillask%==-40 Echo You highly doubt he is your oppoment...
pause
goto cgkill
:crkkill
Echo Your victim lies dead infront of you...
set /a ggp=%RANDOM% %%10 * %level%
echo You gain %ggp% gp.
set /a gp=%gp% + %ggp%
echo You gain %cr% xp.
set /a xp=%xp% + %cr%
pause
Echo The guards enter the crafting room, kicking aside the body of your victim.
Echo They bring you into the battle ring...
pause
goto battle
:craftitem
cls
if %fs3% LSS 1 Echo You don't know how to craft anything!
if %fs3% LSS 1 pause
if %fs3% LSS 1 Goto craft
Echo What do you craft?
if %fs4% GTR 0 Echo 1. A scroll --- Cast a non managed spell depending on scroll and have a small chance of automatic sucsess.
if %fs5% GTR 0 Echo 2. A wand ----- Cast a non managed spell depending on wand with a greater chance of automatic sucsess than a scroll.
if %fs6% GTR 0 Echo 3. A robe ----- Increase AC and hitpoints.
if %fs7% GTR 0 Echo 4. A orb ------ Cast a non managed spell depending on orb with a high chance of automatic sucsess.
Echo 5. Leave
Echo What do you craft?
set /p choice="(1-5)" 
if %choice%==1 goto ct1
if %choice%==2 goto ct2
if %choice%==3 goto ct3
if %choice%==4 goto ct4
if %choice%==5 goto craft
goto craftitem
:ct1
Echo What spell do you put into your scroll?
Echo 1. Firebolt --- Blow fire from your hand to harm your foes
Echo 2. Frostbolt --- Send forth a shard of ice to harm your foes
Echo 3. Waterray --- Blast your foes away dealing minor damage
Echo 4. Faststone --- Create a high accelorated rock harm your foes.
Echo Illusion Spells
Echo 5. Blur --- Make your movements look blurred, granting AC
Echo 6. Daze --- Harm your foe's ability to hit you.
Echo 7. Confusion --- Stun your foe for a turn.
Echo 8. Highlight --- Make your foe glow, making him easier to hit.
Echo Necromancy Spells
Echo 9. Soulcurse --- Gain 50% more XP if sucsessfully hit.
Echo 10. Withercurse --- Deal 2 point of damage per turn if hit and 25% of 1 dex damage.
Echo 11. Heal --- Gain some health.
Echo 12. Sapcurse --- Gain 1 health and deal 1 damage per turn.
set /p choice="Pick a spell."
set /a cspell=%choice%
goto createscroll
:ct2
Echo What spell do you put into your wand?
Echo 1. Firebolt --- Blow fire from your hand to harm your foes
Echo 2. Frostbolt --- Send forth a shard of ice to harm your foes
Echo 3. Waterray --- Blast your foes away dealing minor damage
Echo 4. Faststone --- Create a high accelorated rock harm your foes.
Echo Illusion Spells
Echo 5. Blur --- Make your movements look blurred, granting AC
Echo 6. Daze --- Harm your foe's ability to hit you.
Echo 7. Confusion --- Stun your foe for a turn.
Echo 8. Highlight --- Make your foe glow, making him easier to hit.
Echo Necromancy Spells
Echo 9. Soulcurse --- Gain 50% more XP if sucsessfully hit.
Echo 10. Withercurse --- Deal 2 point of damage per turn if hit and 25% of 1 dex damage.
Echo 11. Heal --- Gain some health.
Echo 12. Sapcurse --- Gain 1 health and deal 1 damage per turn.
set /p choice="Pick a spell."
set /a cspell=%choice%
goto createwand
:ct3
Echo Illusion Spells
Echo 5. Blur --- Make your movements look blurred, granting AC. (Pernament)
Echo Necromancy Spells
Echo 11. Heal --- Gain some health. (Gain extra health)
set /p choice="Pick a spell."
set /a cspell=%choice%
goto createrobe
:ct4
Echo What spell do you put into your orb?
Echo 1. Firebolt --- Blow fire from your hand to harm your foes
Echo 2. Frostbolt --- Send forth a shard of ice to harm your foes
Echo 3. Waterray --- Blast your foes away dealing minor damage
Echo 4. Faststone --- Create a high accelorated rock harm your foes.
Echo Illusion Spells
Echo 5. Blur --- Make your movements look blurred, granting AC
Echo 6. Daze --- Harm your foe's ability to hit you.
Echo 7. Confusion --- Stun your foe for a turn.
Echo 8. Highlight --- Make your foe glow, making him easier to hit.
Echo Necromancy Spells
Echo 9. Soulcurse --- Gain 50% more XP if sucsessfully hit.
Echo 10. Withercurse --- Deal 2 point of damage per turn if hit and 25% of 1 dex damage.
Echo 11. Heal --- Gain some health.
Echo 12. Sapcurse --- Gain 1 health and deal 1 damage per turn.
set /p choice="Pick a spell."
set /a cspell=%choice%
goto createorb
:ct5
goto menu
:createscroll
cls
Echo A scroll will cost %level% crafting materials.
if %level% GTR %magm% Echo You lack the required materials...
if %level% GTR %magm% pause
if %level% GTR %magm% goto craft
set /a magm=%magm% - %level%
Echo How many craft points do you use to your scroll?
set /p power="1-%fs4%"
if %power% gtr %fs4% goto createscroll
Echo You beguin to create the scroll...
pause
set /a craft=%RANDOM% %%19 + 1 + %fs3% + %fs4%
set /a cdc=10 + %level%
set it=scroll
set /a is=%cspell%
if %craft% LSS %cdc% goto cfail
if %cspell%==1 goto csfire
if %cspell%==2 goto cswater
if %cspell%==3 goto csice
if %cspell%==4 goto csrock
if %cspell%==5 goto csblur
if %cspell%==6 goto csdaze
if %cspell%==7 goto csconfuse
if %cspell%==8 goto cshighlight
if %cspell%==9 goto cssoul
if %cspell%==10 goto cswither
if %cspell%==11 goto csheal
if %cspell%==12 goto cssap

:createrobe
cls
Echo A robe will cost %level% crafting materials.
if %level% GTR %magm% Echo You lack the required materials...
if %level% GTR %magm% pause
if %level% GTR %magm% goto craft
set /a magm=%magm% - %level%
Echo How many craft points do you use to your robe?
set /p power="1-%fs6%"
if %power% gtr %fs6% goto createrobe
Echo You beguin to create the robe...
pause
set /a craft=%RANDOM% %%20 + %fs3% + %fs6% + %wis%
set /a cdc=10 + %level% + 5
set it=scroll
set /a is=%cspell%
if %craft% LSS %cdc% goto cfail
if %cspell%==5 goto crblur
if %cspell%==11 goto crheal


:createwand
cls
Echo A wand will cost %level% crafting materials.
if %level% GTR %magm% Echo You lack the required materials...
if %level% GTR %magm% pause
if %level% GTR %magm% goto craft
set /a magm=%magm% - %level%
Echo How many craft points do you use to your wand?
set /p power="1-%fs4%"
if %power% gtr %fs4% goto createwand
Echo You beguin to create the wand...
pause
set /a craft=%RANDOM% %%20 + %fs3% + %fs4%
set /a cdc=10 + %level% + 2
set it=scroll
set /a is=%cspell%
if %craft% LSS %cdc% goto cfail
if %cspell%==1 goto cwfire
if %cspell%==2 goto cwwater
if %cspell%==3 goto cwice
if %cspell%==4 goto cwrock
if %cspell%==5 goto cwblur
if %cspell%==6 goto cwdaze
if %cspell%==7 goto cwconfuse
if %cspell%==8 goto cwhighlight
if %cspell%==9 goto cwsoul
if %cspell%==10 goto cwwither
if %cspell%==11 goto cwheal
if %cspell%==12 goto cwsap

:crblur
set /a is2s=%Tricker% + %power%  + %wis%
set /a is1s=%level% + %power%  + %wis%
set is3s=0
set /a ran= %RANDOM% %%99 + 1
if %ran% gtr 50 set /a is3s=%RANDOM% %%5 + %power%
set is4s=0
set /a ran= %RANDOM% %%99 + 1
if %ran% gtr 50 set /a is4s=%RANDOM% %%5 + %power%
set is5s=0
set /a ran= %RANDOM% %%99 + 1
if %ran% gtr 50 set /a is5s=%RANDOM% %%5 + %power%
set is6s=0
set /a ran= %RANDOM% %%99 + 1
if %ran% gtr 50 set /a is6s=%RANDOM% %%5 + %power%
set is7s=0
set /a ran= %RANDOM% %%99 + 1
if %ran% gtr 50 set /a is7s=%RANDOM% %%5 + %power%
set is8s=0
set /a ran= %RANDOM% %%99 + 1
if %ran% gtr 50 set /a is8s=%RANDOM% %%5 + %power%
set is9s=0
set /a ran= %RANDOM% %%99 + 1
if %ran% gtr 50 set /a is9s=%RANDOM% %%5 + %power%
set is10s=0
set /a ran= %RANDOM% %%99 + 1
if %ran% gtr 50 set /a is10s=%RANDOM% %%5 + %power%
set is11s=0
set /a ran= %RANDOM% %%99 + 1
if %ran% gtr 50 set /a is11s=%RANDOM% %%5 + %power%
set is12s=0
set /a ran= %RANDOM% %%99 + 1
if %ran% gtr 50 set /a is12s=%RANDOM% %%5 + %power%
Echo You crafted the robe!
pause
echo Spell power : %is2s%
echo +%is3s% Begging
echo +%is4s% GatherRenown
echo +%is5s% ReactionSpeed
echo +%is6s% Learner
echo +%is7s% Tricker
echo +%is8s% Empower
echo +%is9s% Imagination
echo +%is10s% Stealth
echo +%is11s% Observer
echo +%is12s% Luck
pause
goto saveitem
:crheal
set /a is2s=%Imagination% + %power%  + %wis%
set /a is1s=%level% + %power%  + %wis%
set is3s=0
set /a ran= %RANDOM% %%99 + 1
if %ran% gtr 50 set /a is3s=%RANDOM% %%5 + %power%
set is4s=0
set /a ran= %RANDOM% %%99 + 1
if %ran% gtr 50 set /a is4s=%RANDOM% %%5 + %power%
set is5s=0
set /a ran= %RANDOM% %%99 + 1
if %ran% gtr 50 set /a is5s=%RANDOM% %%5 + %power%
set is6s=0
