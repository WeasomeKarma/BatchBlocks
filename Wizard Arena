if %bodyhit%==2 set limb=right_arm
if %bodyhit%==3 set limb=left_leg
if %bodyhit%==4 set limb=right_leg
if %bodyhit%==5 set limb=left_hand
if %bodyhit%==6 set limb=right_hand
if %bodyhit%==7 set limb=head
if %bodyhit%==8 set limb=upper torso
if %bodyhit%==9 set limb=lower torso
if %bodyhit%==10 set limb=left_foot
if %bodyhit%==11 set limb=left_eye
if %bodyhit%==12 set limb=right_eye
if %bodyhit%==13 set limb=neck
set /a luckhit=%random% %%10 + %luck%
set /a luckdmg=%luckhit% / %fac%
if %luckdmg% GTR -1 set wound=heavly_brusises
if %luckdmg% GTR 0 set wound=fractures
if %luckdmg% GTR 1 set wound=breaks
if %luckdmg% GTR 2 set wound=shatters
if %luckdmg% GTR 4 set wound=severes
set /a plusdmg=%level%
if %luckdmg% GTR 0 set /a damage= %damage% + %plusdmg%
if %luckdmg% GTR 1 set wound=tears
set /a plusdmg=%level% * 2
if %luckdmg% GTR 1 set /a damage= %damage% + %plusdmg%
if %luckdmg% GTR 2 set wound=mangles
set /a plusdmg=%level% * 3
if %luckdmg% GTR 2 set /a damage= %damage% + %plusdmg%
if %luckdmg% GTR 3 set wound=detaches
set /a plusdmg=%level% * 4
if %luckdmg% GTR 3 set /a damage= %damage% + %plusdmg%
if %luckdmg% GTR 4 set wound=incinerates_to_ash
set /a plusdmg=%level% * 5
if %luckdmg% GTR 4 set /a damage= %damage% + %plusdmg%
Echo You hit %foename% with your spell, it %wound% his %limb%!
if %wound%==Heavly_brusies Echo + %level% damage%!
if %wound%==breaks Echo + %level% * 2 damage!
if %wound%==shatters Echo + %level% * 3 damage!
if %wound%==liquefies Echo + %level% * 4 damage!
if %wound%==severes Echo + %level% * 5 damage!
pause
Echo You deal %damage% damage!
set /a ghp=%ghp% - %damage%
pause
goto guardturn
:gfaststone
Echo You begin to call forth your magical prows to send forth a giant stone to your foe!
pause
set /a chance=%RANDOM% %% 20
set /a chance=%chance% + %Empower% + %int% + %fs1% + %bonus6%
set /a sf=%RANDOM% %% 20 + %gard%
pause
:gsafaststone
echo Empower skill based.
if %chance% gtr %sf% set casted=true
if %chance% lss %sf% goto raspellfail
echo You successfully casted your spell!
set /a damage=%is2s%
if %itemusing%==0 set /a damage=%RANDOM% %% 4 + %Empower% + %level% + %fs1% + %fs13% + %bonus6%
set /a bodyhit=%RANDOM% %%12 + 1
if %bodyhit%==1 set limb=left_arm
if %bodyhit%==2 set limb=right_arm
if %bodyhit%==3 set limb=left_leg
if %bodyhit%==4 set limb=right_leg
if %bodyhit%==5 set limb=left_hand
if %bodyhit%==6 set limb=right_hand
if %bodyhit%==7 set limb=head
if %bodyhit%==8 set limb=upper torso
if %bodyhit%==9 set limb=lower torso
if %bodyhit%==10 set limb=left_foot
if %bodyhit%==11 set limb=left_eye
if %bodyhit%==12 set limb=right_eye
if %bodyhit%==13 set limb=neck
set /a luckhit=%random% %%10 + %luck% + %bonus10%
set /a luckdmg=%luckhit% / %fac%
if %luckdmg% GTR -1 set wound=scrapes
if %luckdmg% GTR 0 set wound=cuts
if %luckdmg% GTR 1 set wound=tears
if %luckdmg% GTR 2 set wound=detaches
if %luckdmg% GTR 4 set wound=pancakes
set /a plusdmg=%level%
set head=head
if %luckdmg% GTR 0 set /a damage= %damage% + %plusdmg%
if %luckdmg% GTR 1 set wound=tears
if %limb%==%head% set /a plusdamage=%plusdmg% * 2 
if %luckdmg% GTR 1 set /a damage= %damage% + %plusdmg%
if %luckdmg% GTR 2 set wound=mangles
if %limb%==%head% set /a plusdamage=%plusdmg% * 2 
if %luckdmg% GTR 2 set /a damage= %damage% + %plusdmg%
if %luckdmg% GTR 3 set wound=detaches
if %limb%==%head% set /a plusdamage=%plusdmg% * 2 
if %luckdmg% GTR 3 set /a damage= %damage% + %plusdmg%
if %luckdmg% GTR 4 set wound=incinerates_to_ash
if %limb%==%head% set /a plusdamage=%plusdmg% * 3 
if %luckdmg% GTR 4 set /a damage= %damage% + %plusdmg%
Echo You hit %foename% with you spell, it %wound% his %limb%!
if %wound%==cuts Echo + %level% damage%!
if %wound%==tears Echo + %level% * 2 damage!
if %wound%==mangles Echo + %level% * 3 damage!
if %wound%==detaches Echo + %level% * 4 damage!
if %wound%==incinerates_to_ash Echo + %level% * 5 damage!
if %limb%==%head% Echo PLUS %level% * 3(head) or 2(other limb)!
pause
Echo You deal %damage% damage!
set /a ghp=%ghp% - %damage%
pause
goto guardturn
:gblur
Echo You begin to cloak yourself in an aura of magical energy to look a-blured.
pause
set /a chance=%RANDOM% %%10
set /a chance=%chance% + %Tricker% + %int% + %fs1% + %bonus5%
set /a sf=%RANDOM% %% 20 + %gard% + 2
pause
echo Empower skill based.
if %chance% gtr %sf% set casted=true
if %chance% lss %sf% goto raspellfail
:gsablur
echo Tricker based spell
echo Your spell is successful!
set /a damage=%is2s%
if %itemusing%==0 set /a blur=%level% + %fs1% + %fs11%
Echo Your AC is improved by %blur% points!
set /a nAC=%nAC% + %blur%
pause
goto guardturn
:gdaze
Echo You begin to invade the mind of your foe... attempting to wound his consousness...
pause
set /a chance=%RANDOM% %%10
set /a chance=%chance% + %Tricker% + %int% + %fs1% + %bonus5%
set /a sf=%RANDOM% %%20 + %gard% +5
echo Imagination skill based.
if %chance% gtr %sf% set casted=true
if %chance% lss %sf% goto raspellfail
:gsadaze
set /a damage=%is2s%
if %itemusing%==0 set /a damage=%RANDOM% %%2 + %level% + %fs1% + %fs11%
echo You successfully casted your spell!
Echo You daze your foe!
Echo You deal %damage% damage!
set /a fhp=%fhp% - %damage%
set /a chance=%RANDOM% %%99+ 1
if %chance% GTR 50 Echo Its your turn again!
pause
if %chance% LSS 50 goto guardturn
goto raplayer
:gconfusion
echo You begin to invade the mind of your foe... attempting to distract his consousness over a long period of time...
set /a chance=%RANDOM% %% 20
set /a damage=%is1s%
if %itemusing%==0 set /a chance=%chance% + %Imagination% + %int% + %fs1% + %bonus7%
set /a sf=%RANDOM% %%20 + %gard% + 8
if %chance% gtr %sf% set casted=true
if %chance% lss %sf% goto raspellfail
:gsaconfusion
echo You successfully casted your spell!
pause
set /a fconf=1
goto guardturn
:ghighlight
set /a chance=%RANDOM% %%20
set /a chance=%chance% + %Tricker% + %int% + %fs1% + %bonus7%
set /a sf=%RANDOM% %% 20 + %gard% + 5
Echo Tricker skill based
Echo You begin to control the flow of magic to your foe to emanate a glowing aura around him...
if %chance% gtr %sf% set casted=true
if %chance% lss %sf% goto raspellfail
:gsahighlight
if %casted%==true echo You successfully casted your spell!
set /a damage=%is2s%
if %itemusing%==0 set /a damage=%RANDOM% %%2 + %level% + %fs11%
set /a =%fac% - %damage%
echo You drop your foe's ac by %damage% points!
pause
goto guardturn
:gsoulcurse
echo You begin to establish a binding with you and your foe's soul, so when he dies your soul will grow...
pause
set /a chance=%is2s%
if %itemusing%==0 set /a chance=%RANDOM% %% 20
if %itemusing%==0 set /a chance=%chance% + %Imagination% + %int% + %fs1% + %bonus7%
set /a sf=%RANDOM% %% 20 + %fac% + 12
if %chance% gtr %sf% set casted=true
if %chance% lss %sf% goto raspellfail
:gsasoulcurse
echo You successfully casted your spell!
Echo Imagination skill based
set /a fsc=1
pause
goto guardturn
:gwithercurse
echo You begin to link the suffering of the dead to the soul of your foe...
pause
set /a chance=%is2s%
if %itemusing%==0 set /a chance=%RANDOM% %% 20
if %itemusing%==0 set /a chance=%chance% + Imagination% + %int% + %fs1% + %bonus7% 
set /a sf=%RANDOM% %% 20 + %gard% + 5
if %chance% gtr %sf% set casted=true
if %chance% lss %sf% goto raspellfail
set /a sdc1=%chance%
:gsawithercurse
echo You successfully casted your spell!
Echo Imagination skill based.
set /a gws=1
Echo Your foe trembles in fear!
pause
goto guardturn
:gheal
echo You begin to use your magical prows to regenerate your wounds...
pause
set /a chance=%is2s%
if %itemusing%==0 set /a chance=%RANDOM% %% 20
if %itemusing%==0 set /a chance=%chance% + %Imagination% + %int% + %fs1% + %bonus7%
set /a sf=%RANDOM% %% 20 + %gard% + 5
if %chance% gtr %sf% set casted=true
if %chance% lss %sf% goto raspellfail
:gsaheal
echo You successfully casted your spell!
if %casted%==true set /a dt= 0
set /a damage=%RANDOM% %% 4 + %Imagination% + %fs1% + %fs15% + %bonus7%
Echo Imagination skill based.
Echo You heal for %damage% hitpoints!
set /a nhp = %nhp% + %damage%
If %nhp% GTR %hp% set /a nhp=%hp%
pause
goto guardturn
:gsapcurse
echo You begin to construct a one way bond from your foe's life force to yours...
pause
set /a damage=%is2s%
if %itemusing%==0 set /a chance=%RANDOM% %%20
if %itemusing%==0 set /a chance=%chance% + %Imagination% + %int% + %fs1% + %bonus7%
set /a sf=%RANDOM% %% 20 + %gard% + 5
if %chance% gtr %sf% set casted=true
if %chance% lss %sf% goto raspellfail
:gsasapcurse
echo You successfully casted your spell!
Echo Imagination skill based.
set /a gsac=1
Echo Your foe trembles in fear!
pause
goto guardturn
:guardturn
cls
if %ghp% LSS 1 goto gdead
if %gws%==1 goto gws
if %gsac%==1 goto gsac
:gact
if %ghp% LSS 1 goto gdead
Echo Its your opponent's turn!
Echo He has %ghp% HP.
pause
goto gmelee
:gws
set /a ran=%RANDOM% %% 100
if %ran% GTR 75 set /a fdex=%fdex% - 1
set /a ghp=%ghp% - %level% - %fs1% - %fs15%
set /a gcast= %RANDOM% %%20 + %cr%
If %gcast% GTR %sdc1% Echo Your foe breaks the withercurse!
If %gcast% GTR %sdc1% pause
If %gcast% GTR %sdc1% set /a gws= 0
goto gact
:gsac
set /a ghp=%ghp% - %level% - %fs1% - %fs15%
set /a nhp=%nhp% + %level% + %fs1% + %fs15%
set /a gcast= %RANDOM% %%20 + %cr%
If %gcast% GTR %sdc1%  Echo Your foe breaks the sapcurse!
If %gcast% GTR %sdc1% pause
If %gcast% GTR %sdc1% set /a %gws% = 0
If %gcast% LSS %sdc1% Echo You feel life energy enter your vessel.
If %gcast% LSS %sdc1% pause
goto gact
:gmelee
cls
Echo The guard roars as he runs at you with his sword in the air!
pause
set /a ghit=%RANDOM% %%20 + %cr%
if %ghit% gtr %nAC% goto ghit
Echo He swings his sword at you but misses!
pause
goto raplayer
:ghit
set /a damage=%RANDOM% %%12 + %cr%
set /a bodyhit=%RANDOM% %%12 + 1
if %bodyhit%==1 set limb=left_arm
if %bodyhit%==2 set limb=right_arm
if %bodyhit%==3 set limb=left_leg
if %bodyhit%==4 set limb=right_leg
if %bodyhit%==5 set limb=left_hand
if %bodyhit%==6 set limb=right_hand
if %bodyhit%==7 set limb=head
if %bodyhit%==8 set limb=upper torso
if %bodyhit%==9 set limb=lower torso
if %bodyhit%==10 set limb=left_foot
if %bodyhit%==11 set limb=left_eye
if %bodyhit%==12 set limb=right_eye
if %bodyhit%==13 set limb=neck
set /a luckhit=%random% %%10 + %cr% -2
set /a luckdmg=%luckhit% / %nAC%
if %luckdmg% GTR -1 set wound=scrapes
if %luckdmg% GTR 0 set wound=slices
if %luckdmg% GTR 1 set wound=shreads
if %luckdmg% GTR 2 set wound=detaches
if %luckdmg% GTR 4 set wound=severes
set /a plusdmg=%cr%
if %luckdmg% GTR 1 set wound=slices
set /a plusdmg=%cr% * 2
if %luckdmg% GTR 1 set /a damage= %damage% + %plusdmg%
if %luckdmg% GTR 2 set wound=shreads
set /a plusdmg=%cr% * 3
if %luckdmg% GTR 2 set /a damage= %damage% + %plusdmg%
if %luckdmg% GTR 3 set wound=detaches
set /a plusdmg=%cr% * 4
if %luckdmg% GTR 3 set /a damage= %damage% + %plusdmg%
if %luckdmg% GTR 4 set severes
set /a plusdmg=%cr% * 5
if %luckdmg% GTR 4 set /a damage= %damage% + %plusdmg%
Echo %foename% hits you with his spell, it %wound% your %limb%!
if %wound%==cuts Echo + %cr% damage%!
if %wound%==slices Echo + %cr% * 2 damage!
if %wound%==shreads Echo + %cr% * 3 damage!
if %wound%==detaches Echo + %cr% * 4 damage!
if %wound%==severes Echo + %cr% * 5 damage!
pause
Echo He deals %damage% damage!
set /a nhp=%nhp% - %damage%
pause
goto raplayer
:raspellfail
Echo You fail to cast your spell!
pause
set /a crcd= 0
set /a condc= 0
set /a chance=%RANDOM% %%100
if %pconf%==0 set /a crcd= 0
if %pconf%==1 set /a condc= %RANDOM% %%3 + %cr%
if %pconf%==1 set /a crcd= %cr%
set /a condc= 80 - %condc%
if %chance% GTR %condc% Echo You deal damage to yourself!
if %chance% GTR %condc% set /a damage= %RANDOM% %%1 + %crcd% + 1
if %chance% GTR %condc% Echo You get %damage% damage!
if %chance% GTR %condc% set /a nhp=%nhp% - %damage%
pause
goto guardturn
:gdead
Echo The guard falls to the ground dead.
pause
Echo Your attempt to run away continues...
pause
cls
goto running
:radead
set /a magm=0
set /a xp=0
set /a gp=0
set /a renown=0
(
echo %magm%
echo %fs1%
echo %fs2%
echo %fs3%
echo %fs4%
