if %hair%==2 set hair=short
if %hair%==3 set hair=non-existant
if %eyesl%==1 set eyesl=large
if %eyesl%==2 set eyesl=small
if %eyesl%==3 set eyesl=wide
if %eyesl%==4 set eyesl=non-existant
