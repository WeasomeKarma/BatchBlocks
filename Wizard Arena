if %AS% GTR 0 goto ability
goto ASdone
:astest4
if %fs3% GTR 0 set /a fs4=%fs4% + 1
if %fs3% LSS 1 Echo You lack the requirements.
if %fs3% LSS 1 pause
if %fs3% LSS 1 goto ability
goto AScheck
:astest5
if %fs3% GTR 2 set /a fs5=%fs5% + 1
if %fs3% LSS 3 Echo You lack the requirements.
if %fs3% LSS 3 pause
if %fs3% LSS 3 goto ability
goto AScheck
