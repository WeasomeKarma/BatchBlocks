if %choice%==5 set /a con=%con% - 2
if %choice%==6 set /a int=%int% - 1
if %choice%==7 set /a int=%int% + 2
if %choice%==1 set race=human
if %choice%==2 set race=orc
if %choice%==3 set race=elf
if %choice%==4 set race=dwarf
if %choice%==5 set race=goblin
if %choice%==5 set race=undead
set /a race=%choice%
set /a hp= %RANDOM% %%8 + %con% + 3
If %hp% lss 1 set /a hp = 2
:racial
cls
set thing=undead
echo ÉÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍ»
